{
  "kind": "collectionType",
  "collectionName": "contactings",
  "info": {
    "name": "contacting"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
